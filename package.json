{

	"name": "Sheet.js",
	"version": "0.0.3alpha",

	"description": "100% DOM-less JavaScript implementation of the styleSheets, cssRule & style APIs\nSupports custom and browser-incompatible CSS syntax like nested rules",
	"keywords": [
		"CSS",
		"SASS",
		"sassy",
		"styleSheet",
		"cssRule",
		"style",
		"parse"
	],

	"implements": [
		"W3C/DOM/styleSheet/2.0",
		"W3C/DOM/cssRule/2.0",
		"W3C/DOM/style/2.0",
		"WebKit/CSS/Animation/1.0"
	],

	"authors": ["Thomas Aylott"],
